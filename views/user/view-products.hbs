            </div><!-- End .col-lg-4 -->
        </div><!-- End .row -->
    </div><!-- End .container -->
</div><!-- End .intro-section -->

<div class="container top mt-5">
    <div class="heading heading-flex mb-3">
        <div class="heading-left">
            <h2 class="title">All Products</h2><!-- End .title -->
        </div><!-- End .heading-left -->
    </div><!-- End .heading -->

    <div class="tab-content tab-content-carousel just-action-icons-sm">
        <div class="tab-pane p-0 fade show active" id="top-all-tab" role="tabpanel" aria-labelledby="top-all-link">
            <div class="owl-carousel owl-full carousel-equal-height carousel-with-shadow" data-toggle="owl"
                data-owl-options='{
                                "nav": true, 
                                "dots": false,
                                "margin": 20,
                                "loop": false,
                                "responsive": {
                                    "0": {
                                        "items":2
                                    },
                                    "480": {
                                        "items":2
                                    },
                                    "768": {
                                        "items":3
                                    },
                                    "992": {
                                        "items":4
                                    },
                                    "1200": {
                                        "items":5
                                    }
                                }
                            }'>
                
         
                {{!-- {{#each wishStatus}} --}}
                {{#each products}}
                <div class="product product-7 text-center">
                    <figure class="product-media">
                        {{#if percentage}}
                        <span class="product-label label-new">-{{this.percentage}}</span>
                        {{/if}}
                        <a href="/productdetails?id={{this._id}}&name={{this.Category}}">
                            <img src="/product-images/{{this._id}}.jpg" alt="Product image" class="product-image">
                        </a>

                        <div class="product-action-vertical">
                          {{#if this.wish}}
                            <a onclick="removeWhish('{{this._id}}')"
                                class="btn-product-icon btn-wishlist btn-expandable" style="color: red;"><span>remove from wishlist</span></a>
                                {{else}}
                                <a onclick="addToWishList('{{this._id}}')"
                                class="btn-product-icon btn-wishlist btn-expandable"><span>add to wishlist</span></a>
                                {{/if}}   
                        </div><!-- End .product-action-vertical -->
                        <div class="product-action">
                            <a onclick="addToCart('{{this._id}}')" class="btn-product btn-cart"><span>add to cart</span></a>
                        </div><!-- End .product-action -->
                    </figure><!-- End .product-media -->
                    <div class="product-body">
                        <div class="product-cat">
                            <a href="#">{{this.Category}}</a>
                        </div><!-- End .product-cat -->
                        <h3 class="product-title"><a href="/productdetails?id={{this._id}}&name={{this.Category}}">{{this.Name}}</a></h3>
                        <!-- End .product-title -->
                        <div class="product-price">
                            {{this.Price}}
                             {{#if actualPrice}}
                            <del class="ml-5 " style="color: black;">â‚¹{{this.actualPrice}}</del>
                            {{/if}}
                        </div><!-- End .product-price -->
                        <div class="product-nav product-nav-thumbs">
                            <a class="active">
                                <img src="/product-images/{{this._id}}.jpg" alt="product desc">
                            </a>
                            <a >
                                <img src="/product-images/{{this._id}}a.jpg" alt="product desc">
                            </a>

                            <a >
                                <img src="/product-images/{{this._id}}b.jpg" alt="product desc">
                            </a>
                        </div><!-- End .product-nav -->
                    </div><!-- End .product-body -->
                </div><!-- End .product -->
                {{/each}}
              
             


            </div><!-- End .product-nav -->
        </div><!-- End .product-body -->
    </div><!-- End .product -->
</div><!-- End .owl-carousel -->

